<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find arna_base_control)/config/arna_base_control.yaml" command="load"/>

  <!-- load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
	output="screen" ns="/arna_base" args="joint_state_controller arna_base_controller"/> <!--ns="/arna_base"-->

  <!-- convert joint states to TF transforms for rviz, etc -->
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"
	respawn="false" output="screen">
    <remap from="/joint_states" to="/arna_base/joint_states" />
  </node> 
  <param name="controller_config" value="$(find arna_base_control)/config/joint_position_controllers.yaml" />
    <node pkg="arna_base_control" type="arm_contoller" name="arm_controller" output="screen" />
</launch>

